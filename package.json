{
  "name": "@supergui/leetcode-cli",
  "version": "3.0.0",
  "description": "A cli tool to enjoy leetcode!",
  "preferGlobal": "true",
  "engines": {
    "node": ">=4"
  },
  "bin": {
    "leetcode": "./bin/leetcode"
  },
  "scripts": {
    "lint": "eslint lib/ test/",
    "test": "npm run lint && nyc mocha test test/plugins && nyc report --reporter=lcov",
    "travis": "node bin/pkg",
    "pkg": "pkg . --out-path=dist/ --targets"
  },
  "pkg": {
    "scripts": [
      "lib"
    ],
    "assets": [
      "colors",
      "icons",
      "templates"
    ],
    "targets": [
      "node10-linux-x64",
      "node10-macos-x64",
      "node10-win-x64"
    ]
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/spgyip/leetcode-cli.git"
  },
  "keywords": [
    "leetcode",
    "cli",
    "command",
    "tool"
  ],
  "author": {
    "name": "Supergui",
    "email": "supergui@live.cn"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spgyip/leetcode-cli/issues"
  },
  "homepage": "https://github.com/spgyip/leetcode-cli#readme",
  "dependencies": {
    "ansi-styles": "^3.2.1",
    "cheerio": "0.22.0",
    "he": "1.2.0",
    "mkdirp": "3.0.1",
    "moment": "^2.30.1",
    "nconf": "0.12.1",
    "ora": "^3.4.0",
    "prompt": "1.3.0",
    "request": "2.88.0",
    "supports-color": "^5.5.0",
    "underscore": "1.13.6",
    "wordwrap": "1.0.0",
    "yargs": "17.7.2"
  },
  "devDependencies": {
    "chai": "5.0.0",
    "eslint": "8.56.0",
    "eslint-config-google": "0.14.0",
    "mocha": "10.2.0",
    "nock": "13.5.0",
    "nyc": "15.1.0",
    "pkg": "^5.8.1",
    "rewire": "7.0.0"
  }
}
